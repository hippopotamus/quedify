<div ng-scope="getMeetings">
<h1>Meetings list</h1>

<ul>
  {{#each collection}}
    <li>
    <h4>{{this.title}}</h4>
    <p><a href="/meetings/{{_id}}">{{_id}}</a></p>
    <p>from: {{this.from}}</p>
    <p>to: {{this.to}}</p>
    <p>location: {{this.location}}</p>
    <p>description: {{this.description}}</p>
    {{this.participants}}
    </li>
  {{/each}}
</ul>
</div>
